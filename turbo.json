{
    "$schema": "https://turbo.build/schema.json",
    "ui": "tui",
    "tasks": {
      "build": {
        "dependsOn": ["^build"],
        "outputs": ["dist/**"]
      },
      "dev": {
        "cache": false,
        "persistent": true
      },
      "test": {
        "outputs": []
      },
      "lint": {
        "outputs": [],
        "dependsOn": []
      },
      "format": {
        "outputs": [],
        "dependsOn": []
      }
    }
  }